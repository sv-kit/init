{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "prettier-plugin-svelte",
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^svelte",
    "^@sveltejs",
    "^\\$app",
    "^\\$env",

    "",

    "<THIRD_PARTY_MODULES>",

    "",

    "^\\~components",
    "^\\~stores",
    "^\\~libs",
    "^\\~utils/(?!types)(.*)$",

    "",

    "^\\~remotes",

    "",

    "^\\~utils/types$",

    "",

    "^\\.\\./\\.\\./\\.\\./",
    "^\\.\\./\\.\\./",
    "^\\.\\./",
    "^\\./",

    "",

    "^\\.\\./\\.\\./\\.\\./",
    "^\\.\\./\\.\\.",
    "^\\.\\.",
    "^\\."
  ],
  "importOrderParserPlugins": ["typescript", "svelte", "decorators-legacy"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
