{
  "name": "yourappname",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "husky": "husky",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "auth:generate": "bun x @better-auth/cli generate --config ./src/libs/auth/index.ts --output ./src/libs/db/tables/auth.ts -y",
    "db:push": "bun --env-file=.env.local -- drizzle-kit push",
    "db:generate": "bun --env-file=.env.local -- drizzle-kit generate",
    "db:migrate": "bun --env-file=.env.local -- drizzle-kit migrate",
    "db:studio": "bun --env-file=.env.local -- drizzle-kit studio",
    "db:generate:prod": "bun --env-file=.env.prod -- drizzle-kit generate",
    "db:migrate:prod": "bun --env-file=.env.prod -- drizzle-kit migrate",
    "db:studio:prod": "bun --env-file=.env.prod -- drizzle-kit studio"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-vercel": "^6.3.1",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^25.2.2",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "commitlint": "^20.4.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.49.2",
    "svelte-check": "^4.3.6",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.4.18",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "mode-watcher": "^1.1.0",
    "zod": "^4.3.6"
  }
}
